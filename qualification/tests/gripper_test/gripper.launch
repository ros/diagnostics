<launch>

  <param name="robotdesc/pr2" command="$(find xacro)/xacro.py '$(find qualification)/tests/gripper_test/gripper.xml'" />

  <machine name="xenomai_root" user="root" address="$(env ROS_TEST_HOST)" ros-root="$(env ROS_ROOT)" ros-package-path="$(env ROS_PACKAGE_PATH)" default="never"/>   


<!-- pr2_etherCAT Needs to be rteth0 if operating in realtime -->
<node machine="xenomai_root" pkg="pr2_etherCAT" type="pr2_etherCAT" args="-i eth0 -x /robotdesc/pr2"/>

<!-- Gripper Calibration -->
<include file="$(find qualification)/tests/upperarm_test/calibrate.launch" />
</launch>
