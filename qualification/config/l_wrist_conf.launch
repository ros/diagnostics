<launch>

<!-- Wrist must have passed qualification, and have been programmed with -wg005 MCB's -->
<!-- Call this program from the localhost, after setting ROS_TEST_HOST -->
<!-- Turn on power, then configure MCB's with 'roslaunch configure_l_wrist.launch -->

 <include file="$(find qualification)/tests/init.machine" />

<!-- configure_mcbs -->
<node machine="realtime" pkg="qualification" type="configure_mcbs.py" args="--motor=l_wrist_l_motor,0 --motor=l_wrist_r_motor,1" />

<!-- service -->
<node pkg="qualification" type="confirm_conf.py" output="screen"/>

</launch>
