<launch>

<!-- Caster must have passed qualification, and have been programmed with -wg005 MCB's -->
<!-- Call this program from the localhost, after setting ROS_TEST_HOST -->
<!-- Turn on power, then configure MCB's with 'roslaunch configure_l_wrist.launch -->

  <include file="$(find qualification)/tests/init.machine" />

<!-- configure_mcbs -->
<node machine="realtime" pkg="qualification" type="configure_mcbs.py" args="--motor=fl_caster_l_wheel_motor,0 --motor=fl_caster_r_wheel_motor,1 --motor=fl_caster_rotation_motor,2" />

<!-- service -->
<node pkg="qualification" type="confirm_conf.py" output="screen"/>

</launch>